.page{
  padding: 0 10px;
}
.header .title{
  font-weight: 800;
  margin: 8px 0;
  font-size: 16px;
}
.header .dec{
  font-size: 14px;
  color: #666;
}
.know{
  width: 100%;
  height: 30px;
  font-size: 12px;
  line-height: 30px;
  border-bottom: 1rpx solid #eee;
  color: #666;
}
.know:hover{
  background-color: #eee;
}
.PracticeQuestions{
  width: 100%;
  height: 500px;
  border-bottom: 1px solid #ccc;
}
.labelName{
  overflow: hidden;
  text-overflow: ellipsis;
  white-space:nowrap;
}
.van-tag--primary{
  margin: 0 3px !important;
}
.description{
  font-size: 16px;
  color: #666;
  padding: 10px 0;
  line-height: 30px;
}
.item{
  padding: 0 10px;
  height: 40px;
  /* border: 1rpx solid #ccc; */
  font-size: 12px;
  color: #666;
  line-height: 40px;
}
.leftIcon{
  float: left;
}
.item .title{
  float: left;
}
.item .icon{
  width: 40px;
  height: 40px;
  float: right;
  text-align: center;
}
.knob{
  font-size: 12px;
  color: #666;
  padding: 0 10px;
  line-height: 40px;
  position: relative;
  margin-left: 20px;
  height: 40px;
}
.knob .title{
  float: left;
}
.knob .icon{
  position: absolute;
  right: 22px;
  bottom: 0;
}
.knob .icon{
  float: right;
}
.knowledgePoint{
  margin-left: 30px;
}
.questionItem{
  margin: 20rpx 0;
  border: 1rpx solid #ccc;
  height: 300rpx;
  background-color: #eee;
  overflow-y: auto;
  padding: 10rpx;
  border-radius: 5px;
}
.question-title{
  font-size: 14px;
  font-weight: 600;
}
.question-select{
  padding: 20rpx 0;
}
.question-analyze{
  font-size: 12px;
  color: #000;
  margin-top: 20rpx;
}